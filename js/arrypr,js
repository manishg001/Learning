// discount for given array

let items = [250, 645, 300, 900, 50];
console.log(`earlier items [${items}]`)

// let idx = 0;
// let val;

// for(val of items) {
//     console.log(`value at idx ${idx} = ${val}`);
//     idx++;
// }

for (let idx = 0; idx < items.length; idx++) {
  let offer = items[idx] / 10;
  items[idx] -= offer;
}

console.log(items);
